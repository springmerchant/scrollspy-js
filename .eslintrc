{
  "env": {
    "browser": true,
    "node": true,
    "amd": true
  },
  "globals": {
    "$": true,
    "jQuery": false
  },
  "rules": {
    "quotes": [1, "single"],
    "strict": [0],
    "max-params": [1, 4],
    "semi": [1, "always"],
    "no-unused-vars": [1, {"vars": "all", "args": "after-used"}],
    "no-alert": [1],
    "brace-style": [2, "1tbs"],
    "no-mixed-spaces-and-tabs": [2, true],
    "no-trailing-spaces": [2],
    "space-after-keywords": [2, "always"],
    "max-nested-callbacks": [1, 3],
    "space-infix-ops": [2],
    "radix": [2],
    "space-unary-ops": [2],
    "wrap-regex": [2],
    "no-mixed-requires": [1, true],
    "eol-last": [0],
    "no-bitwise": [1],
    "dot-notation": [1],
    "no-wrap-func": [0],
    "yoda": [2, "never"],
    "no-nested-ternary": [1],
    "no-delete-var": [1],
    "no-shadow": [2],
    "no-octal": [2],
    "no-extra-bind": [2],
    "no-extend-native": [2],
    "no-underscore-dangle": [0]
  }
}
